package Bootcamp;

import java.util.Scanner;
public class Positiveintegers {
  public static void main(String [] args) {
	  // initialize the values from sum of positive numbers
	  int sum=0;
	  int[]numbers=new int[5];
	  Scanner sc=new Scanner(System.in);
	  //take the input of 5 numbers from console
	  for(int i=0;i<5;i++)
	  {
		  numbers[i]=sc.nextInt();
		  //give all numbers as an input for positive variable
	  }
	  for(int p:numbers)
	  {
		  if(p>0)
		  {
			  sum=sum+p;
		  }
	  }
		  System.out.println("Sum of all positive numbers:" +sum);
	
  }

}

